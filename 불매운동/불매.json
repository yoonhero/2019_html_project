{
  "manifest_version": 2,
  "name": "no 아베"
  "description": "Quick launch don't buy again",
  "version": "1.0.0",
  "content_script": [
    {
      "matches": ["<all_urls>"],
      "js": ["불매운동.js"]
    }
  ]
  "icons": { "128": "image/icon-1.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "불매운동.html"
  },
  "permissions": ["activeTab"]
}
